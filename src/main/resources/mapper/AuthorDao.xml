<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.pg.ljh.dao.AuthorDao">
    <insert id="insertAuthor" parameterType="Author">
        insert into author values(default,#{apenname},#{aname},#{ahead},#{aidentitycard},#{aemail},#{aintroduce},#{phone},null,null,#{apostcode},#{aresidential},now(),default )
    </insert>
</mapper>