<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.pg.ljh.dao.RegisterDao">
    <insert id="registerUser" parameterType="com.pg.ljh.entity.User">
        insert into user values(default,#{uname},#{upassword},null,#{uphone},#{uemail},null,now(),default)
    </insert>
</mapper>